{
  "name": "@privy-io/node",
  "version": "0.2.0",
  "description": "The official TypeScript library for the Privy API API",
  "author": "Privy API <support@privy.io>",
  "types": "./index.d.ts",
  "main": "./index.js",
  "type": "commonjs",
  "repository": "github:privy-io/node-sdk",
  "license": "Apache-2.0",
  "packageManager": "yarn@1.22.22",
  "files": [
    "**/*"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "./scripts/test",
    "test:integration": "jest --testPathPattern=tests/integration --verbose --testTimeout 10000",
    "build": "./scripts/build",
    "format": "./scripts/format",
    "tsn": "ts-node -r tsconfig-paths/register",
    "lint": "./scripts/lint",
    "fix": "./scripts/format"
  },
  "dependencies": {
    "@hpke/chacha20poly1305": "^1.7.1",
    "@hpke/core": "^1.7.4",
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^1.8.0",
    "@scure/base": "^1.2.5",
    "jose": "^6.1.0",
    "canonicalize": "^2.1.0",
    "lru-cache": "^11.1.0"
  },
  "peerDependencies": {
    "viem": "^2.24.1"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "types": "./index.d.mts",
      "default": "./index.mjs"
    },
    "./viem": {
      "require": {
        "types": "./viem.d.ts",
        "default": "./viem.js"
      },
      "types": "./viem.d.mts",
      "default": "./viem.mjs"
    }
  }
}